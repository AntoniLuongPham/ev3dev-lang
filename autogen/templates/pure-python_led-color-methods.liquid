{% comment %}
{% for ledColor in classes.led.ev3LedColors %}
            .def("set_{{ ledColor[0] }}", &ev3::led::set_{{ ledColor[0] }}, args("intensity")).staticmethod("set_{{ ledColor[0] }}")
